<!DOCTYPE html>
<html>
<head>
  <title>Country Population</title>
  <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
  <h1>Country Population</h1>
  <div>
    <label for="filterInput">Filter by country name:</label>
    <input type="text" id="filterInput" placeholder="Search...">
    <br>
    <label for="rangeMin">Min Population (in crore):</label>
    <input type="number" id="rangeMin" min="0" step="0.01">
    <label for="rangeMax">Max Population (in crore):</label>
    <input type="number" id="rangeMax" min="0" step="0.01">
    <button id="filterButton">Filter</button>
    <button id="clearFilter">Clear Filter</button>
  </div>
  <table id="populationTable">
    <thead>
      <tr>
        <th>Country</th>
        <th>Population (September 2021)</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(function(country) { %>
        <tr>
          <td><%= country.Country %></td>
          <td><%= country['Population (September 2021)'] %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  <script src="/js/script.js"></script>
</body>
</html>
